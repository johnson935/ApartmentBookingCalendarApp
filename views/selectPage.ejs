            </a>
            </div>
            
        </div>

 
    <% if (username === "admin"){ %>
        <% users.forEach(function(user){ %>
            <p>  <%= user.username %> last logged in: <%= user.lastLogin %></p>
        <% })} %>
    </div>

    <%- include("partials/footer") %>